<Application x:Class="AutoFlasher.Wpf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AutoFlasher.Wpf">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Zoranof.UI.Wpf;component/Themes/Blue.xaml"/>

                <ResourceDictionary>
                    <local:Bootstrapper x:Key="AppBootstrapper"/>
                </ResourceDictionary>

                <ResourceDictionary Source="pack://application:,,,/Zoranof.UI.Wpf;component/DefaultStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Zoranof.UI.Wpf;component/Resources.xaml"/>

                <ResourceDictionary Source="pack://application:,,,/AutoFlasher.Wpf;component/Resources.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
